; ============================================================
;  PRODUCTION MD --  YAGWIP
; ============================================================
integrator              = md
dt                      = 0.002
nsteps                  = 50000000
nstxout-compressed      = 50000
nstxout                 = 0
nstvout                 = 0
nstfout                 = 0
nstcalcenergy           = 100
nstenergy               = 1000
nstlog                  = 1000
;
cutoff-scheme           = Verlet
nstlist                 = 20
vdwtype                 = Cut-off
vdw-modifier            = None
DispCorr                = EnerPres
rvdw                    = 1.2
rlist                   = 1.2
rcoulomb                = 1.2
coulombtype             = PME
;
constraints             = h-bonds
constraint_algorithm    = LINCS
continuation            = yes
;

